{
    "name": "indatus/guardian",
    "description": "Allows you to protect more than just the production environment from potentially destructive tasks",
    "keywords": ["laravel", "artisan", "migrate", "migrations", "seed", "seeders"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Kuhl",
            "email": "bkuhl@indatus.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/console": "4.2.*@dev",
        "illuminate/database": "4.2.*@dev",
        "illuminate/foundation": "4.2.*@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage": "~1.0@dev"
    },
    "autoload": {
        "psr-0": {
            "Indatus\\Guardian\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
